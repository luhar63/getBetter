{
  "name": "getbetter",
  "productName": "getBetter",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development electron . --enable-logging --remote-debugging-port=9222",
    "electromon": "electromon .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder build --dir",
    "dist": "electron-builder build --x64 --ia32",
    "test": "mocha test --exclude test/main.js",
    "test-e2e": "mocha test/main.js",
    "test-single": "mocha $1",
    "tdd": "mocha --watch test",
    "lint": "./node_modules/.bin/standard",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "keywords": [],
  "author": {
    "name": "Rahul Maurya",
    "email": "rahulmaurya.mnnit@gmail.com"
  },
  "build": {
    "appId": "gameofMinds.getBetter",
    "productName": "Stretchly",
    "dmg": {
      "icon": "build/icon.icns",
      "iconSize": 128,
      "contents": [
        {
          "x": 355,
          "y": 125,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 155,
          "y": 125,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": [
        "dmg"
      ],
      "extendInfo": {
        "LSBackgroundOnly": 1,
        "LSUIElement": 1
      }
    },
    "linux": {
      "category": "Utility",
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "freebsd",
        "pacman",
        "tar.xz",
        "apk"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "nsis-web",
        "7z",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "nsisWeb": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "standard": {
    "globals": [
      "it",
      "describe",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "Audio",
      "fetch",
      "Notification",
      "alert"
    ],
    "ignore": [
      "sun.js"
    ]
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "getBetter"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@meetfranz/electron-notification-state": "^1.0.0",
    "electron-log": "^4.3.2",
    "electron-squirrel-startup": "^1.0.0",
    "i18next": "^19.8.9",
    "i18next-node-fs-backend": "^2.1.3",
    "luxon": "^1.26.0",
    "meeussunmoon": "^3.0.3",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "electromon": "^1.0.10",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "husky": "^5.1.1",
    "mocha": "^8.3.0",
    "pinst": "^2.1.6",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "spectron": "^13.0.0",
    "standard": "^16.0.3"
  }
}
